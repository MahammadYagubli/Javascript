    <script type="text/javascript">
      

        function ad(controlid) {
            var btn = document.getElementById(controlid);
           alert("mouserover");
         btn.style.background = 'blue';
            //btn.style.color = 'yellow';
           
        }
           function da() {
        alert("mouseout");
           yle.background='yellow';
           //   btn.style.color = 'white';
        }
      
        </script>
